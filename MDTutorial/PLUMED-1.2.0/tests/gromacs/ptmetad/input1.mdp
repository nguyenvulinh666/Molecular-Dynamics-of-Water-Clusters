title                    = Folding metadynamics
cpp                      = /lib/cpp
include                  = 
define                   =
tinit                    = 0.0 
integrator               = md 
comm_mode                = none          
nstcomm                  = 10
comm-grps                = System 
dt                       = 0.001
nsteps                   = 100 
; OUTPUT CONTROL OPTIONS =
; Output frequency for coords (x), velocities (v) and forces (f) = 
nstxout                  = 10
nstvout                  = 10
; Output frequency for energies to log file and energy file = 
nstlog                   = 10
nstenergy                = 10 
nstxtcout                = 10 
xtc-precision            = 100000
xtc_grps                 = System 
energygrps               = Protein WAT 
pbc                      = xyz 
nstlist                  = 10
epsilon_r                = 1.
ns_type                  = grid   
coulombtype              = pme    
vdwtype                  = Cut-off
fourierspacing           = 0.12 
; EWALD/PME/PPPM parameters 
pme_order                = 4
ewald_rtol               = 1e-05
epsilon_surface          = 0
optimize_fft             = yes
rlist                    = 0.8
rcoulomb                 = 0.8
rvdw                     = 0.8
tcoupl                   = Nose-Hoover  
tc-grps                  = System 
tau_t                    = 0.05 
ref_t                    = 299.15 
pcoupl                   = Berendsen
pcoupltype               = isotropic
tau_p                    = 0.5 
compressibility          = 4.5e-5
ref_p                    = 1 
; Dielectric constant of reaction field =
epsilon-rf               = 80.0
gen_vel                  = yes  
gen_temp                 = 299.15 
gen_seed                 = 173529
constraints              = all-bonds 
constraint_algorithm     = lincs 
shake_tol                = 0.0001
; Highest order in the expansion of the constraint coupling matrix =
lincs-order              = 4
; Lincs will write a warning to the stderr if in one step a bond =
; rotates over more degrees than =
lincs-warnangle          = 30
user1-grps               = Protein WAT 
; Non-equilibrium MD stuff =
acc-grps                 =
accelerate               =
freezegrps               =  
freezedim                = 
cos-acceleration         = 0 

